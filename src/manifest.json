{
	"manifest_version": 3,
	"author": "Kai Boenke",
	"name": "kbo-Startpage",
	"description": "A personalized startpage.",
	"version": "1.9.5-dev",
	"browser_specific_settings": {
		"gecko": {
			"id": "{0917fcf7-bb50-4b20-af83-274d85af4312}",
			"data_collection_permissions": {
				"required": ["none"],
				"optional": ["none"]
			}
		}
	},
	"data_collection_permissions": "none",
	"icons": {
		"128": "icons/kbo-128.png",
		"96": "icons/kbo-96.png",
		"64": "icons/kbo-64.png",
		"48": "icons/kbo-48.png",
		"32": "icons/kbo-32.png"
	},
	"options_ui": {
		"page": "options.html"
	},
	"chrome_url_overrides": { "newtab": "startpage.html" },
	"chrome_settings_overrides": {
		"homepage": "startpage.html",
		"startup_pages": ["startpage.html"]
	},
	"host_permissions": [
		"https://api.open-meteo.com/*",
		"https://www.reddit.com/*",
		"https://rss.slashdot.org/Slashdot/*",
		"https://www.tagesschau.de/xml/rss2",
		"http://easternsun.de/forum/*",
		"https://fedoramagazine.org/feed/*",
		"https://planet.debian.org/atom.xml",
		"https://www.wowhead.com/news/rss/*",
		"https://bsky.social/*",
		"https://*.bosch.com/*"
	],
	"optional_host_permissions": [
		"geolocation"
	],
	"permissions": ["storage"]
}
