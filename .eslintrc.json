{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "script"
  },
  "extends": "eslint:recommended",
  "plugins": ["jquery"],
  "globals": {
    "chrome": "readonly",
    "$": "readonly",
    "jQuery": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn"
  }
}
